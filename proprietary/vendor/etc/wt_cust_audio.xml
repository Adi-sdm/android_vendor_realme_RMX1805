<mixer>
    <!-- mainmic-->
    <path name="mainmic">
        <ctl name="DEC1 MUX" value="ADC1" />
        <ctl name="ADC1 Volume" value="6" />
        <ctl name="ADC1_INP1 Switch" value="1" />
    </path>

    <!-- mainmic2-->
    <path name="mainmic2">
        <ctl name="DEC1 MUX" value="ADC1" />
        <ctl name="ADC1 Volume" value="6" />
        <ctl name="ADC1_INP1 Switch" value="1" />
    </path>

    <!-- submic-->
    <path name="submic">
        <ctl name="DEC1 MUX" value="ADC1" />
        <ctl name="ADC1 Volume" value="6" />
        <ctl name="ADC1_INP1 Switch" value="1" />
    </path>

    <!-- submic2-->
    <path name="submic2">
        <ctl name="DEC1 MUX" value="ADC1" />
        <ctl name="ADC1 Volume" value="6" />
        <ctl name="ADC1_INP1 Switch" value="1" />
    </path>

    <!-- headset-mic-->
    <path name="headset-mic">
        <ctl name="ADC2 Volume" value="6" />
        <ctl name="DEC1 MUX" value="ADC2" />
        <ctl name="ADC2 MUX" value="INP2" />
    </path>

    <!-- headphone-->
    <path name="headphone">
        <ctl name="MI2S_RX Channels" value="Two" />
        <ctl name="RX1 MIX1 INP1" value="RX1" />
        <ctl name="RX2 MIX1 INP1" value="RX2" />
        <ctl name="RX HPH Mode" value="HD2" />
        <ctl name="COMP0 RX1" value="1" />
        <ctl name="COMP0 RX2" value="1" />
        <ctl name="RDAC2 MUX" value="RX2" />
        <ctl name="HPHL" value="Switch" />
        <ctl name="HPHR" value="Switch" />
    </path>

    <!-- headphoneleft-->
    <path name="headphoneleft">
        <ctl name="MI2S_RX Channels" value="One" />
        <ctl name="RX1 MIX1 INP1" value="RX1" />
        <ctl name="HPHL" value="Switch" />
    </path>

    <!--speaker -->
    <path name="speaker">
        <ctl name="RX3 MIX1 INP1" value="RX1" />
        <ctl name="RX3 Digital Volume" value="84" />
        <ctl name="LINE_OUT" value="Switch" />
        <ctl name="Ext Spk Switch" value="On"/>
    </path>

    <!--speaker right -->
    <path name="speaker-right">
        <ctl name="RX3 MIX1 INP1" value="RX1" />
        <ctl name="RX3 Digital Volume" value="84" />
        <ctl name="LINE_OUT" value="Switch" />
        <ctl name="Ext Spk Switch" value="On"/>
    </path>

    <!-- speakerleft -->
    <path name="speaker-left">
        <ctl name="RX3 MIX1 INP1" value="RX1" />
        <ctl name="RX3 Digital Volume" value="84" />
        <ctl name="LINE_OUT" value="Switch" />
        <ctl name="Ext Spk Switch" value="On"/>
    </path>

    <!-- reciver -->
    <path name="reciver">
        <ctl name="RX1 MIX1 INP1" value="RX1" />
        <ctl name="RDAC2 MUX" value="RX1" />
        <ctl name="RX1 Digital Volume" value="84" />
        <ctl name="EAR PA Gain" value="POS_6_DB" />
        <ctl name="EAR_S" value="Switch" />
    </path>

    <!-- loopback test -->
    <!-- mainmic to headphone -->
    <path name="mmi-loopback-mainmic-headphone">
        <path name="mainmic" />
        <path name="headphone" />
        <ctl name="ADC1 Volume" value="6" />
        <ctl name="RX1 Digital Volume" value="80" />
        <ctl name="RX2 Digital Volume" value="80" />
        <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- mainmic2 to headphone -->
    <path name="mmi-loopback-mainmic2-headphone">
        <path name="mainmic2" />
        <path name="headphone" />
        <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- second to headphone -->
    <path name="mmi-loopback-second-headphone">
       <path name="submic" />
       <path name="headphone" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- second2 to headphone -->
    <path name="mmi-loopback-second2-headphone">
       <path name="submic2" />
       <path name="headphone" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- headset-mic to speaker -->
    <path name="mmi-loopback-hsmic-speaker">
       <path name="headset-mic" />
       <path name="speaker" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- headset-mic to speakerright -->
    <path name="mmi-loopback-hsmic-spkr">
       <path name="headset-mic" />
       <path name="speaker-right" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- headset-mic to speakerleft -->
    <path name="mmi-loopback-hsmic-spkl">
       <path name="headset-mic" />
       <path name="speaker-left" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- headset-mic to receiver -->
    <path name="mmi-loopback-hsmic-receiver">
       <path name="headset-mic" />
       <path name="reciver" />
       <ctl name="RX1 Digital Volume" value="72" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- headset-mic to headphone -->
    <path name="mmi-loopback-hsmic-headphone">
       <path name="headset-mic" />
       <path name="headphone" />
       <ctl name="ADC2 Volume" value="4" />
       <ctl name="RX1 Digital Volume" value="70" />
       <ctl name="RX2 Digital Volume" value="70" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- main-mic to speaker -->
    <path name="mmi-loopback-mainmic-spkr">
       <path name="mainmic" />
       <path name="speaker" />
       <ctl name="ADC1 Volume" value="2" />
       <ctl name="RX2 Digital Volume" value="60" />
       <ctl name="DEC1 Volume" value="60" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- main-mic2 to speaker -->
    <path name="mmi-loopback-mainmic2-spkr">
       <path name="mainmic2" />
       <path name="speaker" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- main-mic to receiver -->
    <path name="mmi-loopback-mainmic-rec">
       <path name="mainmic" />
       <path name="reciver" />
       <ctl name="RX1 Digital Volume" value="72" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- main-mic2 to receiver -->
    <path name="mmi-loopback-mainmic2-rec">
       <path name="mainmic2" />
       <path name="reciver" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- sub-mic to speaker -->
    <path name="mmi-loopback-submic-spkr">
       <path name="submic" />
       <path name="speaker" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- sub-mic to receiver -->
    <path name="mmi-loopback-submic-rec">
       <path name="submic" />
       <path name="reciver" />
       <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>

    <!-- mainmic to headphoneleft -->
    <path name="mmi-loopback-mainmic-headphoneleft">
        <path name="mainmic" />
        <path name="headphoneleft" />
        <ctl name="RX1 Digital Volume" value="70" />
        <ctl name="PRI_MI2S_RX Port Mixer TERT_MI2S_TX" value="1"/>
    </path>
</mixer>
